<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="www.morelab.deusto.es/ontologies/adapta-metadata#"
     xml:base="www.morelab.deusto.es/ontologies/adapta-metadata"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <owl:Ontology rdf:about="www.morelab.deusto.es/ontologies/adapta-metadata"/>   

    <owl:DatatypeProperty rdf:about="www.morelab.deusto.es/ontologies/adapta-metadata#accesible">
        <rdfs:comment>If the dataset is currently accesible</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;boolean"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="www.morelab.deusto.es/ontologies/adapta-metadata#ingoing">
        <rdfs:comment>The number of ingoing links of the described dataset</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="www.morelab.deusto.es/ontologies/adapta-metadata#outgoing">
        <rdfs:comment>The number of outgoing links of the described dataset</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="www.morelab.deusto.es/ontologies/adapta-metadata#inner">
        <rdfs:comment>The number of inner links of the described dataset</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="www.morelab.deusto.es/ontologies/adapta-metadata#links">
        <rdfs:comment>The number of total links of the described dataset</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="www.morelab.deusto.es/ontologies/adapta-metadata#instances">
        <rdfs:comment>The number of total instances of the described dataset</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
</rdf:RDF>

