<?xml version="1.0"?>
<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="www.morelab.deusto.es/ontologies/adapta-metadata#"
     xml:base="www.morelab.deusto.es/ontologies/adapta-metadata"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <owl:Ontology rdf:about="www.morelab.deusto.es/ontologies/adapta-metadata"/>   

    <!-- Data source properties -->

    <owl:DatatypeProperty rdf:about="www.morelab.deusto.es/ontologies/adapta-metadata#accesible">
        <rdfs:comment>If the dataset is currently accesible</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;boolean"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="www.morelab.deusto.es/ontologies/adapta-metadata#ingoing">
        <rdfs:comment>The number of ingoing links of the described dataset</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="www.morelab.deusto.es/ontologies/adapta-metadata#outgoing">
        <rdfs:comment>The number of outgoing links of the described dataset</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="www.morelab.deusto.es/ontologies/adapta-metadata#inner">
        <rdfs:comment>The number of inner links of the described dataset</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="www.morelab.deusto.es/ontologies/adapta-metadata#links">
        <rdfs:comment>The number of total links of the described dataset</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="www.morelab.deusto.es/ontologies/adapta-metadata#instances">
        <rdfs:comment>The number of total instances of the described dataset</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="www.morelab.deusto.es/ontologies/adapta-metadata#ld-stars">
        <rdfs:comment>Number of stars according to http://www.w3.org/DesignIssues/LinkedData.html</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    
    <!-- End of data source properties -->
    
    <!-- Vocabulary properties -->
    
    <owl:Class rdf:about="www.morelab.deusto.es/ontologies/adapta-metadata#Vocabulary">
        <rdfs:comment>Represents a vocabulary and its related information</rdfs:comment>
    </owl:Class>
    
    <owl:DatatypeProperty rdf:about="www.morelab.deusto.es/ontologies/adapta-metadata#reference">
        <rdfs:comment>URL which refers to the vocabulary</rdfs:comment>
        <rdfs:domain rdf:resource="www.morelab.deusto.es/ontologies/adapta-metadata#Vocabulary"/>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="www.morelab.deusto.es/ontologies/adapta-metadata#quality">
        <rdfs:comment>The quality of the vocabulary</rdfs:comment>
        <rdfs:domain rdf:resource="www.morelab.deusto.es/ontologies/adapta-metadata#Vocabulary"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="www.morelab.deusto.es/ontologies/adapta-metadata#valid_syntax">
        <rdfs:comment>Vocabulary syntax is correct</rdfs:comment>
        <rdfs:domain rdf:resource="www.morelab.deusto.es/ontologies/adapta-metadata#Vocabulary"/>
        <rdfs:range rdf:resource="&xsd;boolean"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="www.morelab.deusto.es/ontologies/adapta-metadata#usage">
        <rdfs:comment>Number of other known datasets using this vocabulary</rdfs:comment>
        <rdfs:domain rdf:resource="www.morelab.deusto.es/ontologies/adapta-metadata#Vocabulary"/>
        <rdfs:range rdf:resource="&xsd;boolean"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="www.morelab.deusto.es/ontologies/adapta-metadata#integration">
        <rdfs:comment>Number of other known datasets which could integrate through this vocabulary</rdfs:comment>
        <rdfs:domain rdf:resource="www.morelab.deusto.es/ontologies/adapta-metadata#Vocabulary"/>
        <rdfs:range rdf:resource="&xsd;boolean"/>
    </owl:DatatypeProperty>

    <!-- End of vocabulary properties -->
    
    <!-- Data section -->
    
    <owl:DatatypeProperty rdf:about="www.morelab.deusto.es/ontologies/adapta-metadata#data-syntax">
        <rdfs:comment>Dataset syntax is correct</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;boolean"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="www.morelab.deusto.es/ontologies/adapta-metadata#access-numbers">
        <rdfs:comment>Number of access to the dataset</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="www.morelab.deusto.es/ontologies/adapta-metadata#broken-links">
        <rdfs:comment>Number of broken links the dataset contains</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="www.morelab.deusto.es/ontologies/adapta-metadata#integrity-errors">
        <rdfs:comment>Number of integrity errors dataset contains</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    
    <!-- End of data section -->
    
    <!-- Provenance section -->
    
    <owl:DatatypeProperty rdf:about="www.morelab.deusto.es/ontologies/adapta-metadata#has_provenance">
        <rdfs:comment>Dataset contains provenance info described using an standard vocabulary</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;boolean"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="www.morelab.deusto.es/ontologies/adapta-metadata#distance">
        <rdfs:comment>Distance to dataset original source</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="www.morelab.deusto.es/ontologies/adapta-metadata#source_quality">
        <rdfs:comment>Quality of the data source</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="www.morelab.deusto.es/ontologies/adapta-metadata#has_certificate">
        <rdfs:comment>URL which refers to the vocabulary</rdfs:comment>
        <rdfs:domain rdf:resource="www.morelab.deusto.es/ontologies/adapta-metadata#Vocabulary"/>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="www.morelab.deusto.es/ontologies/adapta-metadata#asProvider">
        <rdfs:comment>Number of references to other known datasets as data provider</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="www.morelab.deusto.es/ontologies/adapta-metadata#asConsumer">
        <rdfs:comment>Number of references to other known datasets where data is consumed from</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
        
    <!-- End of provenance section -->
    
</rdf:RDF>

